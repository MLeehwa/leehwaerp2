{
    "version": 2,
    "builds": [
        {
            "src": "backend/src/server.mongodb.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/src/server.mongodb.ts"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}